#include <iostream>
using namespace std;
int main() {
    int a[5]; 
    int sum = 0;
    for(int i = 0; i < 5; i++)
    {
        cin >> a[i];
    }
    for(int i = 0; i < 5; i++)
    {
        cout << "The number " << i << " is: " << a[i] << ' ' << endl;
    }
    int max = a[0];
    for(int i = 0; i < 5; i++)
    {
      if(a[i] > max)  
      {
        max = a[i];
      }
    }
    int min = a[0]; 
    for(int i = 0; i < 5; i++)
    {
      if(a[i] < min)  
      {
        min = a[i];
      }
    }
    for(int i = 0; i < 5; i++) 
    {
     sum += a[i];   
    }
    for(int i = 0; i < 5; i++)
    {
        cout << "The reversed number " << i << " is: " << a[4 - i] << ' ' << endl;
    }

    int cent1 = 0;
    int cent2 = 4;
    int x[5] = {6, 7, 8, 9, 10};
    for(int i = 0; i < 5; i++)
    {
        int temp = x[cent1];
        x[cent1] = x[cent2];
        x[cent2] = temp;
        cent1++;
        cent2--;
    }
    for(int i = 0; i < 5; i++) 
    {
        cout << "The number " << i << " is: " << x[i] << ' ' << endl;
    }
    cout << "The minimum number: " << min << endl;
    cout << "The maximum number: " << max << endl;
    cout << "The sum of numbers: " << sum << endl;

}
